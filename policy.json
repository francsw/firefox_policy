{
  "policies": {
    "Authentication": {
      "SPNEGO": ["*.ops.cert.govt.nz"],
      "Delegated": ["*.ops.cert.govt.nz"],
      "AllowNonFQDN": {
        "SPNEGO": true,
        "NTLM": false
      },
      "AllowProxies": {
        "SPNEGO": false,
        "NTLM": false
      },
      "Locked": false,
      "PrivateBrowsing": true
    },
    "PasswordManagerEnabled": false,
    "DisableFormHistory": true,
    "DisableFirefoxAccounts": true,
    "DisablePocket": true,
    "DisableFirefoxStudies": true,
    "DisableTelemtry": true,
    "BlockAboutAddons": true,
    "FirefoxHome": {
        "Highlights": false,
        "Pocket": false
    },
    "Homepage": {
      "URL": "https://jira.int.{{mail_domain}}/",
      "StartPage": "homepage",
      "Locked": true
    },
    "NoDefaultBookmarks": true,
    "Proxy": {
      "AutoLogin": true,
      "HTTPProxy": "proxy.ext.ops.cert.govt.nz:3128",
      "Mode": "manual",
      "Passthrough": "localhost,.int.ops.cert.govt.nz,.int.uat.ops.cert.govt.nz,.adm.ops.cert.govt.nz",
      "UseHTTPProxyForAllProtocols": true,
      "UseProxyForDNS": false
    },
    "DisplayBookmarksToolbar": true
  }
}
