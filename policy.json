{
  "policies": {
    "Authentication": {
      "SPNEGO": ["*.ops.cert.govt.nz"],
      "Delegated": ["*.ops.cert.govt.nz"],
      "AllowNonFQDN": {
        "SPNEGO": true,
        "NTLM": false
      },
      "AllowProxies": {
        "SPNEGO": false,
        "NTLM": false
      },
      "Locked": false,
      "PrivateBrowsing": true
    },
    "PasswordManagerEnabled": false,
    "DisableFormHistory": true,
    "DisableFirefoxAccounts": true,
    "DisablePocket": true,
    "DisableFirefoxStudies": true,
    "DisableTelemtry": true,
    "BlockAboutAddons": true,
    "FirefoxHome": {
        "Highlights": false,
        "Pocket": false
    },
    "Homepage": {
      "URL": "https://jira.int.{{mail_domain}}/",
      "StartPage": "homepage",
      "Locked": true
    },
    "NoDefaultBookmarks": true
    "Proxy": {
      "AutoLogin": true,
      "Mode": "system",
      "SOCKSVersion": 5,
      "UseHTTPProxyForAllProtocols": true,
      "UseProxyForDNS": false
    }
  }
}
